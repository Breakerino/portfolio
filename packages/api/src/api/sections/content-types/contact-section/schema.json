      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "email": {
      "type": "string",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      }
    },
    "socials": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "github",
        "linkedin",
        "facebook",
        "instagram",
        "spotify",
        "youtube",
        "myanimelist"
