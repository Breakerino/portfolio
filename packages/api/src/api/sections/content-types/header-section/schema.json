{
  "kind": "singleType",
  "collectionName": "header_section_settings",
  "info": {
    "singularName": "header-section",
    "pluralName": "header-section-settings",
    "displayName": "Sections / Header"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "i18n": {
      "localized": true
    }
  },
  "attributes": {
    "settingsName": {
      "type": "string",
      "private": true,
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "default": "Sections / Header",
      "unique": true
    },
    "logo": {
      "type": "media",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "navigation": {
      "type": "component",
      "pluginOptions": {
        "i18n": {
          "localized": true
        }
      },
      "component": "site.navigation",
      "repeatable": true
    },
		"socials": {
      "type": "customField",
      "pluginOptions": {
        "i18n": {
          "localized": false
        }
      },
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "github",
        "linkedin",
        "facebook",
        "instagram",
        "spotify",
        "youtube",
        "myanimelist"
      ]
    }
  }
}
